{
    "expo": {
        "name": "LocalePlate",
        "slug": "LocalePlate",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/icon@3x.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true
        },

        "plugins": ["expo-font"]
    }
}
